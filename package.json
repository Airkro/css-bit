{
  "name": "css-bit-dev",
  "version": "0.0.29",
  "license": "MIT",
  "devDependencies": {
    "ant-design-palettes": "^1.1.3",
    "fs-extra": "^7.0.0",
    "lodash": "^4.17.11",
    "node-sass": "^4.9.3"
  },
  "optionalDependencies": {
    "prettier-stylelint": "^0.4.2",
    "stylelint": "^9.5.0",
    "stylelint-config-airkro": "^1.1.5"
  },
  "scripts": {
    "prebuild": "stylelint package/src/**/*.{css,scss} --fix --s scss",
    "build": "node-sass package/index.scss package/dist/index.css && node-sass package/reboot.scss package/dist/reboot.css",
    "postbuild": "prettier-stylelint package/dist/*.css --write -q"
  }
}
